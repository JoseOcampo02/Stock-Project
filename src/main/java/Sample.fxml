<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SampleController">
   <top>
      <AnchorPane prefHeight="1376.0" prefWidth="1373.0" BorderPane.alignment="CENTER">
         <children>
            <LineChart id="macdChart" fx:id="lineChart" layoutX="31.0" layoutY="461.0" onMouseMoved="#handleMouseMoved" prefHeight="361.0" prefWidth="658.0" title="MACD">
              <xAxis>
                <NumberAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
               <cursor>
                  <Cursor fx:constant="CROSSHAIR" />
               </cursor>
            </LineChart>
            <LineChart id="priceDataline" fx:id="priceChart" layoutX="38.0" layoutY="38.0" onMouseMoved="#handleMouseMoved" prefHeight="391.0" prefWidth="669.0" title="Price Data">
              <xAxis>
                <NumberAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
               <cursor>
                  <Cursor fx:constant="CROSSHAIR" />
               </cursor>
            </LineChart>
            <Label fx:id="hoverLabel" layoutX="38.0" layoutY="14.0" />
            <Label fx:id="hoverLabel2" layoutX="38.0" layoutY="418.0" />
         </children>
      </AnchorPane>
   </top>
</BorderPane>
